<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="main_application" class="cdeler.ide.Ide">
        <constructor-arg index="0" value="800"/>
        <constructor-arg index="1" value="600"/>
        <constructor-arg index="2" value="/icons8-ruby-programming-language-64.png"/>
        <constructor-arg index="3" value="Ruby Pit"/>
    </bean>

    <bean id="jni_tokenizer" class="cdeler.highlight.JNITokenizer"/>

</beans>